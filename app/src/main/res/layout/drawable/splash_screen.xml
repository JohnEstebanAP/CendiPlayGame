<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/white"/>

    <item android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/green_700"/>
            <size android:width="230dp" android:height="230dp"/>
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/green_500"/>
            <size android:width="230dp" android:height="200dp"/>
        </shape>
    </item>

    <item android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <size android:width="180dp" android:height="230dp"/>
        </shape>
    </item>


    <item
        android:gravity="center"
        android:width="150dp"
        android:height="150dp">
        <bitmap android:src="@drawable/mushroom"/>
    </item>
</layer-list>